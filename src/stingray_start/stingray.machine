<launch>
  
  <!-- Basestation Computer -->
  <machine name="basestation" address="stingray-basestation" default="never" />

  <!-- Vision Computer -->
  <machine name="vision" user="stingray-vision" address="stingray-vision"
      default="true" />

  <!-- Controls Computer -->
  <machine name="control" user="stingray-control" address="stingray-control" />

</launch>
